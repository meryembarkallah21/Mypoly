<form method="post" action="{{ path('semestre_delete', {'id': semestre.id}) }}" onsubmit="return confirm('Vous etes sure que vous voulez supprimer cet item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ semestre.id) }}">
    <a class="btn btn-sm btn-success" href="{{ path('semestre_edit', {'id': semestre.id}) }}"><span class="fas fa-edit"></span></a>
    <button class="btn btn-sm btn-danger"><i class="fa fw fa-trash-alt"></i></button>
</form>
