{# empty Twig template #}
    <!-- Modal -->
    <div id="resinscription" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <form action="#" method="post" class="form-horizontal form-label-left was-validated">

                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header bg-info">
                        <h4 class="modal-title">Réinscription</h4>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div class="x_panel">

                            <div class="x_content" style="display: block;">
                                <br>
                                    <div class="form-group row">
                                        <label class="col-sm-4 col-form-label" for="Année scolaire">Année Scolaire <span class="required">*</span>
                                        </label>
                                        <div class="col-sm-8">
                                            <input id="" name="" class="form-control" type="text" value="{{annees.year}}" readonly/>
                                            <input id="annee" type="hidden" name="annee" class="form-control" value="{{annees.id}}"/>

                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="control-label col-sm-4" for="classe">Classe <span class="required">*</span>
                                        </label>
                                        <div class="col-sm-8">
                                            <select id="gestetablisbundle_inscrire_Classe" name="classe" class="form-control" required>
                                                <option value="">--Choisir mention--</option>
                                                {%for  item in mentions %}
                                                    <option value="{{item.id}}">{{item.name}}</option>
                                                {%endfor%}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-sm-4 col-form-label" for="matricule">N° Matricule <span class="required">*</span>
                                        </label>
                                        <div class="col-sm-8">
                                            <input type="text" name="eleve" class="form-control" placeholder="numéro matricule" required />
                                        </div>
                                    </div>

                            </div>
                        </div>
                    </div>
                    <div class="modal-footer justify-content-between">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Femer</button>
                        <button type="submit" class="btn btn-danger">Enregistrer</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <!--endmodal-->

